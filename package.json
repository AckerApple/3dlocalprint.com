{
  "name": "3dlocalprint",
  "version": "1.0.10",
  "description": "[gh-pages display](https://ackerapple.github.io/3dlocalprint/)",
  "private": true,
  "type": "module",
  "scripts": {
    "build:html": "node scripts/build-html.js",
    "dev": "npm run build:html && vite",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "bump": "npm version patch --no-git-tag-version",
    "build": "npm run build:html && vite build",
    "preview": "vite preview",
    "deploy": "npm run bump && npm run build && git add . && git commit -m \"deploy\" && git push"
  },
