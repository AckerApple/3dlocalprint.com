  "bugs": {
    "url": "https://github.com/AckerApple/3dlocalprint/issues"
  },
  "homepage": "https://github.com/AckerApple/3dlocalprint#readme",
  "devDependencies": {
    "@types/node": "^24.3.1",
    "typescript": "^5.9.2",
    "vite": "^6.4.1"
  },
  "dependencies": {
    "firebase": "^12.7.0",
    "qr-scanner": "^1.4.2",
    "taggedjs": "file:../../web/taggedjs/main/dist"
  }
